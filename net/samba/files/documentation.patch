+          "opened_at": "2022\-04\-15T17:30:38+0200"
+        }
+      }
+    }
+  }
+}
+				
+.fi
+.if n \{\
+.RE
+.\}
+.sp
+If oplocks are used:
+.sp
+.if n \{\
+.RS 4
+.\}
+.nf
+          "oplock": {
+            "EXCLUSIVE": false,
+            "BATCH": false,
+            "LEVEL_II": true,
+            "LEASE": false,
+            "text": "LEVEL_II"
+          }
+				
+.fi
+.if n \{\
+.RE
+.\}
+.sp
+If leases are used:
+.sp
+.if n \{\
+.RS 4
+.\}
+.nf
+          "lease": {
+            "lease_key": "29316055\-f55c\-de10\-c813\-af7bf5a430bb",
+            "hex": "0x00000005",
+            "READ": true,
+            "WRITE": true,
+            "HANDLE": false,
+            "text": "RW"
+          }
+			
+.fi
+.if n \{\
+.RE
+.\}
+.sp
+With byte\-range locks (\-B, \-\-byterange):
+.sp
+.if n \{\
+.RS 4
